{
	"name": "monolayer-demo",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack",
		"prebuild": "npx prisma generate",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"db:bootstrap": "npm run db:migrate && npm run db:seed",
		"db:seed": "npx tsx ./app/_lib/db/seed.ts",
		"db:migrate": "tsx ./app/_lib/db/migrate.ts",
		"db:studio": "dotenv -f .env.local prisma studio"
	},
	"dependencies": {
		"@monolayer/sdk": "^1.0.0",
		"@next/env": "^15.4.2",
		"@prisma/client": "^6.12.0",
		"lorem-ipsum": "^2.0.8",
		"lucide-react": "^0.511.0",
		"next": "^15.4.1",
		"prisma": "^6.12.0",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"reshaped": "^3.6.3",
		"tsx": "^4.19.4",
		"uuid": "^11.1.0",
		"@aws-sdk/client-s3": "^3.844.0",
		"@aws-sdk/s3-request-presigner": "^3.844.0",
		"@heroicons/react": "^2.2.0"
	},
	"devDependencies": {
		"@aws-sdk/client-dynamodb": "^3.844.0",
		"@eslint/eslintrc": "^3",
		"@tailwindcss/postcss": "^4",
		"@types/node": "^20",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"dotenv": "^16.5.0",
		"esbuild": "^0.25.6",
		"eslint": "^9",
		"eslint-config-next": "15.3.2",
		"prettier": "^3.5.3",
		"prettier-plugin-organize-imports": "^4.1.0",
		"tailwindcss": "^4",
		"typescript": "^5"
	}
}
